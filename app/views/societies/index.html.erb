

<h1>Membresías</h1>


<div class="btn-group" role="group" aria-label="...">
  <%= link_to "<i class='fa fa-plus-circle'></i> Nueva Membresía".html_safe,
              new_society_path, class: 'btn btn-primary btn-lg'%>



</div>
<div class="btn-group" role="group" aria-label="...">   <%= link_to "Reportes Membresía".html_safe,               reportmembresias_path , class: 'btn btn-primary btn-lg'%>    </div>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripción</th>
      <th>Cuota anual</th>

      <th>Couta mensual</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @societies.each do |society| %>
      <tr>
        <td><%= society.nombre %></td>
        <td><%= society.descripcio %></td>
        <td><%= society.valor_anual %></td>

        <td><%= society.valor_mensual %></td>



        <td><%= link_to 'Ver', society,class:"btn btn-primary btn-xs" %></td>
        <td><%= link_to 'Editar', edit_society_path(society), class:"btn btn-success btn-xs" %></td>
        <td><%= link_to 'Eliminar',society, method: :delete, data: { confirm: 'Eliminar?' }, class:"btn btn-danger btn-xs" %></td>


      </tr>
    <% end %>
  </tbody>
</table>

<br>




